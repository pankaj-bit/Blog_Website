<%- include('partials/header') -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="usr">Title</label>
        <input type="text" class="form-control" name="postTitle" required id="usr" autocomplete="off">
    </div>
    <div class="form-group">
        <label for="post">Post</label>
        <textarea class="form-control" name="postBody" rows="5" id="post" value="adc" required></textarea>
    </div>
         
      <button class="btn btn-primary" type="submit" name="button" > Publish</button>
</form>


<%- include('partials/footer') -%>